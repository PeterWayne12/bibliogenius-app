# BiblioGenius App Environment Configuration
# Copy this file to .env and adjust values as needed

# Backend API URL (for HTTP mode, not used in FFI mode)
API_BASE_URL=http://localhost:8000

# BiblioGenius Hub URL (for feedback, sync, etc.)
# HUB_URL=http://localhost:8081

# Enable search result caching (true/false)
ENABLE_SEARCH_CACHE=true

# Show developer tools in help screen (true/false)
# SHOW_DEV_TOOLS=false
